file = open('Hello.txt', 'w') #파일을 열 때 w:쓰기 모드로 열기
file.write('Hello,World!!' 'Test with python.') #파일 쓰기
file.close() #파일 닫기

file = open('hello.txt','r') #파일을 열 때 r:읽기 모드로 열기
s=file.read() #파일을 읽는다
print(s)
file.close() #파일을 닫는다

file = open('Hello.txt', 'w')
file.wirte('Hello, Python!!!!!' 'Wellcome to Python')
file = close()
