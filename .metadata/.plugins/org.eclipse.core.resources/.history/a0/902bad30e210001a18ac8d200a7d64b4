a = [1,2,3]
b = [4,5,6]
c = a+b
print(c)

a = (1,2,3)
b = (4,5,6)
c = a+b
print(a+b)

# 7 번 째의 글자를 출력하기
hello = "according to old saying, great hope make great man"
a=list(hello)
print(a)
print(a[7])
print(len(hello))

# 인덱스
a=[0,0,0,0,0]
a[0] = 11
a[1] = 22
a[2] = 33
a[3] = 44
a[4] = 55
print(a) 

# 튜플 인덱싱
b = tuple(range(0,300,7))
print(b)
print(b[0])
print(b[3:8]) # 3~8번 째 까지의 수를 출력
len(b) # 문자열의 길이를 출력
print(b[42])
print(b[35:])# 35번 째 이후로 모두 출력
print(b[-1]) # 42번 째 출력
print(b[-10]) # 리버스 10번 째 출력
print(b[-42]) # 리버스 42번 째 출력
print(b[:7:2]) # 0 부터 7번째 까지 숫자를 출력 후 2개의 간격으로 출력

s = slice(4,8) # s에 4~8번 째 까지의 배열을 저장
print(b[s])

c = list(range(0,40,2))
print(c)
h = c[2:7]
print(h)
h[0] = '즐'
h[1] = '거'
h[2] = '운'
h[3] = '수'
h[4] = '업'
print(h)
t=c+h
print(t)