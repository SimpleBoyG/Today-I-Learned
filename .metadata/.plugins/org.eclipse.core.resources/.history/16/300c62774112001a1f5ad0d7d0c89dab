print('1.')
a = [[10,20],
     [30,20],
     [50,30]]
print(a[0][0])
print(a[1][0])
print(a[2][1])


print('\n2.')
for i in a:
    print(i)
    for j in i:
        print(j, end=' ')
    print()
   
#=========아직 이해 못함 ============================= 
print('\n3.')    
for i in range(len(a)):
    for j in a[i]:
        print(j, end=' ')
    print()
    
    
a = []
for i in range(10):
    a.append(0)
print(a)

b=[[],[]]
for i in range(4):
    b[0].append(0)
    print(b)
    for j in range(5):
        b[1].append(0)
        print(b)

    



