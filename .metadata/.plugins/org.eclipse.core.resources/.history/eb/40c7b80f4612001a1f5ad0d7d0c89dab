print('1.')
a = [[10,20],
     [30,20],
     [50,30]]
print(a[0][0])
print(a[1][0])
print(a[2][1])


print('\n2.')
for i in a:
    print(i)
    for j in i:
        print(j, end=' ')
    print()
   
#=========아직 이해 못함 ============================= 
print('\n3.')    
for i in range(len(a)):
    for j in a[i]:
        print(j, end=' ')
    print()
    
    
a = []
for i in range(10):
    a.append(0)
print(a)

#================ 2차원 배열 생성 =====================
b = [] # 배열 선언
for i in range(5):
    c = []
    for j in range(4):
        c.append(0)
    b.append(c)
print(b[0])
print(b[1])
print(b[2])
print(b[3])
print(b[4])
#================ 2차원 배열 생성2(한줄로) ======================
a = [[0 for j in range(2)] for i in range(3)]
print(a)
b = [[1 for j in range(2)] for i in range(4)] 
#================ 2차원 배열 생성3(한줄로) ======================
a = [[5] * 2 for i in range(3)]
print(a)



    



