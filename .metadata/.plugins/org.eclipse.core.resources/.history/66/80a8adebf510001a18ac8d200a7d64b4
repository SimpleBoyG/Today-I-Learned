    print(b-a, end='')
    print('a = ', end='')
    print('b = ', end='')
    print('difference is ', end ='')
    print(b-a)
    
if a > c :
    print('a is ', end ='')
    print(a-c, end = '')
    print('much more higher then ', end = '')
    print(' value of c.')
    
